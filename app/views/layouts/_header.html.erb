<header>
  <div class="header-cont">
    <ul>
      <li><%= link_to "Acquaintnote", root_path, id: "logo" %></li>
    </ul>
    <nav>
      <ul>
        <li><%= link_to "Friends", friends_path %></li>
        <% if user_signed_in? %>
        		<li><%= link_to "Sign out", destroy_user_session_path, method: "delete" %></li>
        	<% else %>
        		<li><%= link_to "Sign in", user_session_path %></li>
            <li><%= link_to "Sign in with Facebook", user_facebook_omniauth_authorize_path %></li>
            <li><%= link_to "Sign in with Twitter", user_twitter_omniauth_authorize_path %></li>
        	<% end %>
      </ul>
    </nav>
  </div>
</header>
